@model IEnumerable<UserStoreName>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="row">
    <div class="col-2"></div>
    <div class="col-2 text-center">
        <h2 class="text-primary">Stores List </h2>
    </div>
    <div class="col-8"></div>
</div>
<div class="row">
    <div class="col-8"></div>
    <div class="col-2 text-center">
        <a class="btn btn-primary" asp-action="Index" style="background-color:darkslateblue  "><i class="fas fa-eye"></i> &nbsp; Admin Stores</a>
    </div>
    <div class="col-2"></div>
</div>

<br />
<div class="container p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>User</th>
                <th>Store Name</th>
                <th></th>
        </thead>
    </table>
</div>
@section Scripts{
    <script src="~/js/AllUsersStore.js"></script>
}


